<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<template id="my_profile" inherit_id="portal.portal_my_home">
			<xpath expr="//div[hasclass('o_portal_category')]/t[2]" position="after">
				<t t-call="portal.portal_docs_entry">
					<t t-set="icon" t-value="'/student/static/description/icon.png'" />
					<t t-set="title">
						Redeem Point
					</t>
					<t t-set="text">
						Redeem products with poing
					</t>
					<t t-set="url" t-value="'/redeem'" />
					<t t-set="config_card" t-value="True" />
				</t>
			</xpath>
		</template>
	</data>
</odoo>
